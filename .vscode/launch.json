{
	"version": "0.2.0",
	"configurations": [
	  {
		"type": "node",
		"request": "launch",
		"name": "Launch Extension",
		"skipFiles": [
		  "<node_internals>/**"
		],
		"program": "${workspaceFolder}/node_modules/.bin/vscode",
		"args": [
		  "extensions",
		  "run"
		],
		"runtimeExecutable": null,
		"runtimeArgs": [],
		"env": {
		  "VSCODE_EXTENSIONS_DIR": "${workspaceFolder}/.vscode/extensions"
		},
		"outFiles": [
		  "${workspaceFolder}/dist/**/*.js"
		],
		"preLaunchTask": "npm: compile"
	  }
	]
  }
  